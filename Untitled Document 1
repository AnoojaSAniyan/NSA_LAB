shell programming

echo -e "menu \n 1.circle \n 2.rectangle \n 3.triangle"
echo "enter your choice: "
read ch 
case $ch in
1)echo "enter raduis"
read r
ar=`expr 22 \* $r \* $r / 7`
echo " area:$ar ";;
2)echo "enter length"
read l
echo "enter breadth"
read b
ar=`expr $l \* $b`
echo "area:$ar";;
3)echo "enter height"
read h
echo "enter breadth"
read c
ar=`expr $h \* $c / 2`
echo "area:$ar";;
esac
output 

